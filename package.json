		"tailwindcss": "^4.0.8",
		"tsx": "^4.19.2",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"unplugin-icons": "^22.1.0",
		"vite": "^6.0.0"
	},
	"dependencies": {
		"@prisma/client": "^6.4.1",
		"ai": "^4.1.45",
		"humanize-duration": "^3.32.1",
		"ollama-ai-provider": "^1.2.0",
		"seedrandom": "^3.0.5"
	}
