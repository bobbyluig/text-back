		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@tailwindcss/vite": "^4.0.8",
		"@types/cli-progress": "^3.11.6",
		"@types/humanize-duration": "^3.27.4",
		"@types/node": "^22.13.4",
		"@types/seedrandom": "^3.0.8",
		"cli-progress": "^3.12.0",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^2.46.1",
